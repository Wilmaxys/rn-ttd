{
  "expo": {
    "name": "TimeToDoux",
    "slug": "rn-ttd",
    "version": "0.1.4",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "timetodoux",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timetodoux.ttd"
    }
  }
}
